{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run prototyping/height.pde",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "${config:processing.path}",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      },
      "args": [
        "--force",
        "--sketch=${workspaceRoot}/prototyping/height",
        "--output=${workspaceRoot}/prototyping/height/out",
        "--run"
      ],
      "windows": {
        "args": [
          "--force",
          "--sketch=${workspaceRoot}\\prototyping\\height",
          "--output=${workspaceRoot}\\prototyping\\height\\out",
          "--run"
        ]
      },
      "problemMatcher": []
    },
    {
      "label": "Run prototyping/biomes.pde",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "${config:processing.path}",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      },
      "args": [
        "--force",
        "--sketch=${workspaceRoot}/prototyping/biomes",
        "--output=${workspaceRoot}/prototyping/biomes/out",
        "--run"
      ],
      "windows": {
        "args": [
          "--force",
          "--sketch=${workspaceRoot}\\prototyping\\biomes",
          "--output=${workspaceRoot}\\prototyping\\biomes\\out",
          "--run"
        ]
      }
    }
  ]
}
